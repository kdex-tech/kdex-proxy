{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Remote Debug Proxy",
            "type": "go",
            "request": "attach",
            "mode": "remote",
            "cwd": "${workspaceFolder}",
            "port": 2345,
            "host": "localhost",
            "trace": "log",
            "logOutput": "rpc",
        },
        {
            "name": "Launch Proxy",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "cmd/main.go",
            "env": {
                "CONFIG_FILE": "${workspaceFolder}/test/resources/config1.yaml",
                "MODULE_DIR": "${workspaceFolder}/test/modules",
                "UPSTREAM_ADDRESS": "main.dxp.docker.localhost",
                "UPSTREAM_HEALTHZ_PATH": "/c/portal/robots"
            }
        },
        {
            "name": "Scan Packages",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "cmd/scanner/main.go",
            "cwd": "${workspaceFolder}",
            "env": {
                "MODULE_DIR": "${workspaceFolder}/tmp/node_modules",
            }
        }
    ]
}